<h1>登录</h1>
 
<% if flash[:login_error] %>
  <p><%= flash[:login_error] %></p>
<% end %>
 
<%= form_for :session, url: :sessions_create1 do |f| %>
 
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :password_confirm %>
    <%= f.password_field :password_confirm %>
  </p>
 
  <p><%= f.button "登录" %></p>
 
<% end %>
