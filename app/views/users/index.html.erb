<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>password_degist</th>
      <th>password_confirm</th>
      <th>key</th>
      <th>beiwanglu</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% for user in @users %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.password_digest %></td>
        <td><%= user.password_confirm %></td>
        <td><%= user.key %></td>
        <td><%= user.user_beiwanglu%></td>
      </tr>
    <% end %>
  </tbody>
</table>

